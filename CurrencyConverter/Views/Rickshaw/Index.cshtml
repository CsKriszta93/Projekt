@Model CurrencyConverter.Models.Money

<!DOCTYPE html>
<html>
<head runat="server">
    <title>Hello word</title>
</head>
<body>

    @foreach (var curr in Model)
    {
        <div>
            <input type="checkbox" id="@curr" name="feature"
                   value="@curr" checked />
            <label for="@curr">@curr</label>
        </div>
        @*@Html.CheckBoxFor(false)*@
        @*<option value="@curr.Currency">@curr.Currency</option>*@
    }


    <script src="Views/Home/d3.v2.js"></script>
    <script src="Views/Home/rickshaw.min.js"></script>

    <div id="chart"></div>

    <script>

			var graph = new Rickshaw.Graph({
				element: document.querySelector("#chart"),
				width: 235,
				height: 85,
				renderer: 'line',
				series: [{
					//data: [ { x: 0, y: 40 }, { x: 1, y: 49 }, { x: 2, y: 38 }, { x: 3, y: 30 }, { x: 4, y: 32 } ],
					data: @Model,
					color: '#4682b4'
				}, {
					data: [ { x: 0, y: 20 }, { x: 1, y: 24 }, { x: 2, y: 19 }, { x: 3, y: 15 }, { x: 4, y: 16 } ],
					color: '#9cc1e0'
				}]
			});
			graph.render();

    </script>

</body>
</html>